{
    "id": "a-trust",
    "name": "A-Trust",
    "description": "Clearly authenticate your customers with a mobile phone signature on the web.",
    "version": "1.0",
    "type": "connector",
    "vendor": "Axon Ivy AG",
    "platformReview": "4.5",
    "sourceUrl": "https://github.com/axonivy-market/a-trust-connector",
    "language": "English",
    "industry": "Cross-Industry",
    "compatibility": "9.2+",
    "tags": [ "e-signature" ],
    "mavenArtifacts": [
        {
            "name": "A-Trust",
            "groupId": "com.axonivy.connector.atrust",
            "artifactId": "a-trust-connector",
            "makesSenseAsMavenDependency": true
        },
        {
            "name": "A-Trust Demo",
            "groupId": "com.axonivy.connector.atrust",
            "artifactId": "a-trust-connector-demo"
        }
    ],
    "installers": [
        {
            "id": "maven-dependency",
            "data": {
                "dependencies": [
                    {
                        "groupId": "com.axonivy.connector.atrust",
                        "artifactId": "a-trust-connector",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories": [
                    {
                        "id": "axonivy.public",
                        "url": "http://repo.axonivy.rocks",
                        "snapshots": {
                            "enabled": "true"
                        }
                    }
                ]
            }
        },
        {
            "id": "maven-import",
            "data": {
                "projects": [
                    {
                        "groupId": "com.axonivy.connector.atrust",
                        "artifactId": "a-trust-connector-demo",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories": [
                    {
                        "id": "axonivy.public",
                        "url": "http://repo.axonivy.rocks"
                    }
                ]
            }
        },
        {
            "id": "open-resource",
            "data": {
                "project": "a-trust-connector-demo",
                "resources": [
                    "processes/ATrustIntegration/ATrustDemo.mod"
                ]
            }
        }
    ]
}
