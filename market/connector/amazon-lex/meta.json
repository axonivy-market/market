{
    "id": "amazon-lex",
    "name": "Amazon Lex Runtime API",
    "description": "Amazon Lex is a NLP service to build sophisticated, natural language, conversational bots (chatbots).",
    "version": "1.0",
    "type": "connector",
    "vendor": "Axon Ivy AG",
    "platformReview": "4.5",
    "sourceUrl": "https://github.com/axonivy-market/amazon-lex-connector",
    "statusBadgeUrl": "https://github.com/axonivy-market/amazon-lex-connector/actions/workflows/ci.yml/badge.svg",
    "language": "English",
    "industry": "Cross-Industry",
    "compatibility": "9.2+",
    "tags": [ "AI" ],
    "validate": false,
    "mavenArtifacts": [
        {
            "repoUrl": "https://maven.axonivy.com",
            "name": "Amazon Lex",
            "groupId": "com.axonivy.connector.amazon.lex",
            "artifactId": "amazon-lex-connector",
            "makesSenseAsMavenDependency": true
        },
        {
            "repoUrl": "https://maven.axonivy.com",
            "name": "Amazon Lex Demo",
            "groupId": "com.axonivy.connector.amazon.lex",
            "artifactId": "amazon-lex-connector-demo"
        }
    ],
    "installers": [
        {
            "id": "maven-dependency",
            "data": {
                "dependencies": [
                    {
                        "groupId": "com.axonivy.connector.amazon.lex",
                        "artifactId": "amazon-lex-connector",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories": [
                    {
                        "id": "axonivy.public",
                        "url": "https://maven.axonivy.com",
                        "snapshots": {
                            "enabled": "true"
                        }
                    }
                ]
            }
        },
        {
            "id": "maven-import",
            "data": {
                "projects": [
                    {
                        "groupId": "com.axonivy.connector.amazon.lex",
                        "artifactId": "amazon-lex-connector-demo",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories": [
                    {
                        "id": "axonivy.public",
                        "url": "https://maven.axonivy.com"
                    }
                ]
            }
        }
    ]
}
