{
    "installers": [
        {
            "id": "maven-dependency",
            "data": {
                "dependencies": [
                    {
                        "groupId": "com.axonivy.connector.amazon.lex",
                        "artifactId": "amazon-lex-connector",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories": [
                    {
                        "id": "axonivy.public",
                        "url": "https://maven.axonivy.com",
                        "snapshots": {
                            "enabled": "true"
                        }
                    }
                ]
            }
        },
        {
            "id": "maven-import",
            "data": {
                "projects": [
                    {
                        "groupId": "com.axonivy.connector.amazon.lex",
                        "artifactId": "amazon-lex-connector-demo",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories": [
                    {
                        "id": "axonivy.public",
                        "url": "https://maven.axonivy.com"
                    }
                ]
            }
        }
    ]
}
