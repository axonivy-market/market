{
	"id": "msgraph",
	"name": "Microsoft 365 (Preview)",
	"tags": ["connector", "outlook"],
	"properties": {
		"tags": "me.user,me.calendar,users.calendar,me.message,me.Actions,me.todo",
		"scopes": "User.Read Calendars.ReadWrite Mail.ReadWrite Mail.Send Tasks.ReadWrite",
		"main": ""
	},
	"installers": [
		{
			"...": "<project-res.json>"
		},
		{
			"...": "<rest-client.json>"
		},
		{
			"id": "project-resource",
			"data": {
				"directories": [
					{
						"source": "demo/msGraphDemo/processes/Demo",
						"includes": [ 
							"ms365ToDo.mod", 
							"ms365Calendar.mod",
							"ms365Mail.mod"
						],
						"path": "processes/Demo"
					},
					{
						"source": "demo/msGraphDemo/dataclasses/ms/graph/demo",
						"includes": [ 
							"ToDoDemo.ivyClass",
							"CalendarDemo.ivyClass",
							"MailDemo.ivyClass"
						],
						"path": "dataclasses/ms/graph/demo"
					}
				]
			}
		},
		{
			"id": "open-resource",
			"data": {
				"resources": [
					"processes/Demo/ms365ToDo.mod",
					"processes/Demo/ms365Mail.mod",
					"processes/Demo/ms365Calendar.mod"
				]
			}
		}
	]
