                    "ch.ivyteam.ivy.rest.client.mapper.JsonFeature"
                ],
                "properties": [
                    {
                        "name": "AUTH.tenant",
                        "value": "AXONPRESALES"
                    },
                    {
                        "name": "AUTH.clientId",
                        "value": ""
                    },
                    {
                        "name": "AUTH.userKey",
                        "value": "",
