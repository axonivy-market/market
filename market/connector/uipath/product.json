    },
    "installers": [
        {
            "id": "project-resource",
            "data": {
                "directories": [
                    {
                        "source": "demo/uiPathDemo/src/com/uipath/orchestrator/feature",
                        "includes": [
                            "TenantHeaderFeature.java",
                            "OAuth2Feature.java"
                        ],
                        "path": "src/com/uipath/orchestrator/feature"
                    }
                ],
                "resources": [
                    {
                        "source": "demo/uiPathDemo/webContent/icons/uipath.png",
