                    },
                    {
                        "name": "JSON.Mapper.ACCEPT_CASE_INSENSITIVE_PROPERTIES",
                        "value": "true"
                    }
                ]
            }
        },
        {
            "id": "project-resource",
            "data": {
                "resources": [
                    {
