{
	"id": "tel-search-connector",
	"name": "tel.search.ch",
	"description": "It has never been easier to retrieve phone directory entries via the tel.search.ch API.",
	"type": "solution",
	"vendor": "Axon Ivy AG",
	"platformReview": "4.5",
	"sourceUrl": "https://github.com/axonivy-market/tel-search-connector",
	"statusBadgeUrl": "https://github.com/axonivy-market/tel-search-connector/actions/workflows/ci.yml/badge.svg",
	"language": "English",
	"industry": "Cross-Industry",
	"compatibility": "8.0+",
	"tags": ["helper"],
	"validate": false,
	"mavenArtifacts": [
		{
			"repoUrl": "https://maven.axonivy.com",
			"name": "Tel Search Connector",
			"groupId": "com.axonivy.connector.telsearch",
			"artifactId": "tel-search-connector",
			"makesSenseAsMavenDependency": true
		},
		{
			"repoUrl": "https://maven.axonivy.com",
			"name": "Tel Search Connector Demo",
			"groupId": "com.axonivy.connector.telsearch",
			"artifactId": "tel-search-connector-demo"
		}
	],
	"installers": [
		{
			"id": "maven-import",
			"data": {
				"projects": [
					{
						"groupId": "com.axonivy.connector.telsearch",
						"artifactId": "tel-search-connector-demo",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories": [
					{
						"id": "axonivy.public",
						"url": "https://maven.axonivy.com",
						"snapshots": {
							"enabled": "true"
						}
					}
				]
			}
		},
				{
			"id": "maven-dependency",
			"data": {
				"dependencies": [
					{
						"groupId": "com.axonivy.connector.telsearch",
						"artifactId": "tel-search-connector",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories": [
					{
						"id": "axonivy.public",
						"url": "https://maven.axonivy.com",
						"snapshots": {
							"enabled": "true"
						}
					}
				]
			}
		}
	]
}
