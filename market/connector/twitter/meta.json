{
	"id": "twitter",
	"name": "Twitter",
	"tags": ["connector"],
	"installers": [
		{
			"id": "project-resource",
			"data": {
				"directories": [
					{
						"source": "demo/twitterDemo/src/com/twitter/auth",
						"includes": [ 
							"OAuth2Feature.java"
						],
						"path": "src/com/twitter/auth"
					}
				],
				"resources": [
					{
						"source": "demo/twitterDemo/icons/twitter.png",
						"path": "icons/twitter.png"
					}
				]
			}
		},
		{
			"id": "rest-client",
			"data": {
				"openApiUrl": "https://api.twitter.com/2/openapi.json",
				"namespace": "com.twitter",
				"icon": "res:/icons/twitter.png",
				"clientId": "0b74edb8-5459-4ccc-aac7-505531e06398",
				"features": [
					"ch.ivyteam.ivy.rest.client.mapper.JsonFeature",
					"com.twitter.auth.OAuth2Feature"
				],
				"properties": [
					{
						"name": "API.key"
					},
					{
						"name": "API.secretKey",
						"password": true
					},
					{
						"name": "AUTH.baseUri",
						"value": "https://api.twitter.com/oauth2"
					}
				]
			}
		},
		{
			"id": "project-resource",
			"data": {
				"directories": [
					{
						"source": "demo/twitterDemo/src_hd/com/twitter/demo/Tweets",
						"includes": [ 
							"TweetsData.ivyClass", 
							"Tweets.rddescriptor", 
							"TweetsProcess.mod", 
							"Tweets.xhtml"
						],
						"path": "src_hd/com/twitter/demo/Tweets"
					},
					{
						"source": "demo/twitterDemo/webContent/layouts",
						"includes": [ 
							"basic-8.xhtml", 
							"styles/ivy-common-8.css", 
							"images/axon_ivy_logo.svg"
						],
						"path": "webContent/layouts"
					}
				],
				"resources": [
					{
						"source": "demo/twitterDemo/dataclasses/com/twitter/demo/Data.ivyClass",
						"path": "dataclasses/com/twitter/demo/Data.ivyClass"
					},
					{
						"source": "demo/twitterDemo/processes/Demo/twitter.mod",
						"path": "processes/Demo/twitter.mod"
					}
				]
			}
		},
		{
			"id": "open-resource",
			"data": {
				"resources": [
					"processes/Demo/twitter.mod"
				]
			}
		}
	]
}
