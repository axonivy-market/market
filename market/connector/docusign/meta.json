{
	"id": "docusign",
	"name": "DocuSign eSignature",
	"tags": ["connector"],
	"installers": [
		{
			"id": "maven-dependency",
			"data": {
				"dependencies": [
					{
						"groupId": "com.docusign",
						"artifactId": "docusign-esign-java",
						"version": "3.10.1",
						"exclusions": [ { 
							"groupId": "*", 
							"artifactId": "*" 
						} ]
					},
					{
						"groupId": "io.jsonwebtoken",
						"artifactId": "jjwt",
						"version": "0.9.1",
						"exclusions": [ { 
							"groupId": "*", 
							"artifactId": "*" 
						} ]
					},
					{
						"groupId": "org.bouncycastle",
						"artifactId": "bcprov-jdk15on",
						"version": "1.60"
					}
				]
			}
		},
		{
			"id": "project-resource",
			"data": {
				"directories": [
					{
						"source": "demo/eSignatureDemo/src/net/docusign",
						"includes": [ 
							"auth/UserUriFilter.java", 
							"auth/OAuth2Feature.java", 
							"auth/JwtFactory.java",
							"event/EnvelopeCompleted.java",
							"DocUtils.java",
							"SignUtils.java"
						],
						"path": "src/net/docusign"
					},
					{
						"source": "demo/eSignatureDemo/cms/redirect",
						"includes": [ 
							"co.meta",
							"1766AE730F59FBF6.jsp"
						],
						"path": "cms/redirect"
					}
				],
				"resources": [
					{
						"source": "demo/eSignatureDemo/icons/docusign.png",
						"path": "icons/docusign.png"
					}
				]
			}
		},
		{
			"id": "rest-client",
			"data": {
				"openApiUrl": "https://raw.githubusercontent.com/docusign/eSign-OpenAPI-Specification/master/esignature.rest.swagger-v2.1.json",
				"namespace": "com.docusign.esign.model",
				"icon": "res:/icons/docusign.png",
				"clientId": "17e5dc27-5699-4717-ae6c-e2c8f1e2a30f",
				"skipCodegen": true,
				"features": [
					"ch.ivyteam.ivy.rest.client.mapper.JsonFeature",
					"net.docusign.auth.OAuth2Feature"
				],
				"properties": [
					{
						"name": "AUTH.integrationKey",
						"value": "your-app-key"
					},
					{
						"name": "AUTH.secretKey",
						"password": true
					},
					{
						"name": "AUTH.systemUserId"
					},
					{
						"name": "AUTH.systemKeyFile",
						"value": "docusign.pem"
					},
					{
						"name": "AUTH.scope",
						"value": "signature impersonation"
					},
					{
						"name": "AUTH.baseUri",
						"value": "https://account-d.docusign.com/oauth"
					},
					{
						"name": "JSON.Serialization.PROPERTY_INCLUSION",
						"value": "NON_NULL"
					}
				]
			}
		},
		{
			"id": "project-resource",
			"data": {
				"resources": [
					{
						"source": "demo/eSignatureDemo/dataclasses/net/docusign/esignature/demo/Data.ivyClass",
						"path": "dataclasses/net/docusign/esignature/demo/Data.ivyClass"
					},
					{
						"source": "demo/eSignatureDemo/processes/Demo/eSign.mod",
						"path": "processes/Demo/eSign.mod"
					}
				],
				"directories": [
					{
						"source": "demo/eSignatureDemo/src_hd/net/docusign/esignature/demo",
						"includes": [ 
							"DocDlg/DocDlgData.ivyClass", 
							"DocDlg/DocDlg.rddescriptor", 
							"DocDlg/DocDlgProcess.mod", 
							"DocDlg/DocDlg.xhtml",

							"DocSign/DocSignData.ivyClass", 
							"DocSign/DocSign.rddescriptor", 
							"DocSign/DocSignProcess.mod", 
							"DocSign/DocSign.xhtml",

							"DocUpload/DocUploadData.ivyClass", 
							"DocUpload/DocUpload.rddescriptor", 
							"DocUpload/DocUploadProcess.mod", 
							"DocUpload/DocUpload.xhtml"
						],
						"path": "src_hd/net/docusign/esignature/demo"
					},
					{
						"source": "demo/eSignatureDemo/webContent/layouts",
						"includes": [ 
							"basic-8.xhtml", 
							"styles/ivy-common-8.css", 
							"images/axon_ivy_logo.svg"
						],
						"path": "webContent/layouts"
					}
				]
			}
		},
		{
			"id": "open-resource",
			"data": {
				"resources": [
					"processes/Demo/eSign.mod"
				]
			}
		}
	]
}
