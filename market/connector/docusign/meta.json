{
	"id": "docusign",
	"name": "DocuSign eSignature",
	"listed": false,
	"tags": ["connector"],
	"installers": [
		{
			"id": "rest-client",
			"data": {
				"openApiUrl": "https://github.com/docusign/eSign-OpenAPI-Specification/raw/master/esignature.rest.swagger-v2.1.json",
				"namespace": "net.docusign.esignature",
				"clientId": "17e5dc27-5699-4717-ae6c-e2c8f1e2a30f",
				"ignoreCompileErrors": true,
				"features": [
					"ch.ivyteam.ivy.rest.client.mapper.JsonFeature",
					"net.docusign.auth.OAuth2Feature"
				],
				"properties": [
					{
						"name": "AUTH.integrationKey",
						"value": "your-app-key"
					},
					{
						"name": "AUTH.secretKey",
						"value": "",
						"password": true
					},
					{
						"name": "AUTH.baseUri",
						"value": "https://account-d.docusign.com/oauth"
					}
				]
			}
		}
	]
}
